[Unit]
Description=Minecraft log forwarder
After=network.target

[Service]
Type=simple
ExecStart=/home/zztkm/.deno/bin/deno run --allow-read --allow-net /home/zztkm/genkai-mc-server/logForwarder.ts /home/zztkm/genkai-mc-server/log_forwarder_config.json
Restart=on-failure

[Install]
WantedBy=default.target